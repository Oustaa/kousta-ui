{
  "name": "kousta-ui",
  "version": "0.1.6",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "demos/*",
      "docs"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "build": "lerna run build",
    "clean": "lerna run clean",
    "publish": "lerna publish from-package",
    "lint": "eslint '**/src/*.{js,ts,tsx}'",
    "format": "prettier --write 'packages/**/*.{ts,tsx,json,css,md}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev": "yarn lerna run dev",
    "components:demo": "cd ./demos/components-demo && yarn run dev",
    "table:demo": "cd ./demos/table-demo && yarn run dev",
    "demos": "concurrently \"yarn run components:demo\" \"yarn run table:demo\"",
    "knip": "knip",
    "docs:start": "yarn workspace docs start",
    "docs:build": "yarn workspace docs build",
    "docs:serve": "yarn workspace docs serve"
  },
  "keywords": [
    "ui-library",
    "react"
  ],
  "author": "Oussama Tailba",
  "license": "ISC",
  "description": "",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/config-lerna-scopes": "^19.7.0",
    "@docusaurus/theme-live-codeblock": "^3.9.1",
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/addon-onboarding": "^8.3.3",
    "@storybook/blocks": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@storybook/test": "^8.3.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.10",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "hash-css-selector": "^1.1.0",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lerna": "8.2.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "postcss-modules": "^6.0.1",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.79.4",
    "storybook": "^8.3.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "packageManager": "yarn@4.5.0",
  "dependencies": {
    "knip": "^5.63.1"
  }
}
